{
  "name": "suppress-eslint-errors",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Faithlife/suppress-eslint-errors"
  },
  "author": "David Mitchell <dave@fallingcanbedeadly.com>",
  "license": "MIT",
  "description": "A codemod that suppresses existing violations of eslint rules.",
  "engines": {
    "node": ">= 10.12.0"
  },
  "bin": {
    "suppress-eslint-errors": "./bin/index.js"
  },
  "files": [
    "bin/index.js",
    "transforms/*.js"
  ],
  "scripts": {
    "suppress-eslint-errors": "node bin/index.js",
    "lint": "eslint bin transforms",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "faithlife"
    ]
  },
  "release": {
    "branch": "master"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "jscodeshift": "^0.6.3",
    "please-upgrade-node": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "commitlint-config-faithlife": "^1.0.0",
    "eslint": "^5.14.1",
    "eslint-config-faithlife": "^4.8.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "typescript": "^3.3.3"
  }
}
